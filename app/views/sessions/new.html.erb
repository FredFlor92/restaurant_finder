<h1> Log in Here! </h1>

<p><%flash[:error]%></p>
 
<%= form_for :user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
    <%= f.submit "Log in" %> 
<% end %>

<%= button_to 'Login with Google', "auth/google_oauth2" %>